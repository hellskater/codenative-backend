{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "paths": {
        "@api/*": ["api/*"],
        "@config/*": ["config/*"],
        "@setup/*": ["setup/*"]
      },
    "baseUrl": "src"
  },
  "module": {
    "type": "commonjs"
  }
}
